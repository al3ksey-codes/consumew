{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "functions": {
        "api/dispatch_messages.py": {
            "cron": "*/1 * * * *"
        },
        "api/ping_nodes.py": {
            "cron": "*/5 * * * *"
        }
    }
}
